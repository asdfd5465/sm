<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <exclude domain="database" path="user_profile.db" />
        <exclude domain="database" path="content.db" />
        <!-- Exclude any other user-specific or large files -->
    </cloud-backup>
    <device-transfer>
        <exclude domain="database" path="user_profile.db" />
        <exclude domain="database" path="content.db" />
    </device-transfer>
</data-extraction-rules>
